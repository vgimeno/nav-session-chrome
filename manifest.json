{
    "manifest_version": 2,
    "name": "Nav Session Chrome",
    "description": "This extension stores the current opened tabs and let assign a session name for futher opening",
    "version": "1.0",
    "browser_action": {
        "default_icon": "img/icon-128.png",
        "default_popup": "popup.html",
        "default_title": "Save your tabs"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}